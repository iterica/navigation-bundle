{
  "name": "iterica/navigation-bundle",
  "type": "library",
  "description": "Symfony navigation bundle",
  "homepage": "https://github.com/iterica/navigation-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Riekelt Kapitein",
      "email": "riekelt@iterica.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "iterica/navigation-core": "^0.2",
    "symfony/config": "~4.0 || ^5.0",
    "symfony/dependency-injection": "~4.0 || ^5.0",
    "symfony/expression-language": "~4.0 || ^5.0",
    "symfony/http-foundation": "~4.0 || ^5.0",
    "symfony/http-kernel": "~4.0 || ^5.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.8.2",
    "ergebnis/license": "^1.1.0",
    "ergebnis/test-util": "^1.1.0",
    "friendsofphp/php-cs-fixer": "~2.16.4",
    "infection/infection": "~0.17.3",
    "jangregor/phpstan-prophecy": "~0.8.0",
    "phpstan/extension-installer": "^1.0.5",
    "phpstan/phpstan": "~0.12.40",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpstan/phpstan-strict-rules": "~0.12.5",
    "phpunit/phpunit": "^9.5.7",
    "symfony/routing": "^5.1.5",
    "symfony/security-core": "~4.0 || ^5.0",
    "symfony/translation": "^5.1.5",
    "twig/twig": "~2.0 || ~3.0"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Iterica\\NavigationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iterica\\NavigationBundle\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/iterica/navigation-bundle/issues",
    "source": "https://github.com/iterica/navigation-bundle"
  }
}
