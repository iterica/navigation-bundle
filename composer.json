{
  "name": "iterica/navigation-bundle",
  "type": "library",
  "description": "Symfony navigation bundle",
  "homepage": "https://github.com/iterica/navigation-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Riekelt Kapitein",
      "email": "riekelt@iterica.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "iterica/navigation-core": "dev-master",
    "symfony/dependency-injection": "^4.0 || ^5.0",
    "symfony/config": "~4.0 || ^5.0",
    "symfony/http-kernel": "~4.0 || ^5.0",
    "symfony/security-core": "~4.0 || ^5.0",
    "symfony/expression-language": "~4.0 || ^5.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.6.1",
    "ergebnis/license": "^1.0.0",
    "ergebnis/php-cs-fixer-config": "^2.2.1",
    "ergebnis/phpstan-rules": "~0.15.0",
    "ergebnis/test-util": "^1.0.0",
    "infection/infection": "~0.15.3",
    "jangregor/phpstan-prophecy": "~0.8.0",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "~0.12.36",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.15",
    "phpstan/phpstan-strict-rules": "~0.12.4",
    "phpunit/phpunit": "^9.2.6",
    "psalm/plugin-phpunit": "~0.10.1",
    "vimeo/psalm": "^3.13.1"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Iterica\\NavigationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iterica\\NavigationBundle\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/iterica/navigation-bundle/issues",
    "source": "https://github.com/iterica/navigation-bundle"
  }
}
